#!/opt/tsmart-date/tbash
. $(dirname "$0")/lib-common

[[ "$#" == 0 ]] && log-error "No configure command" && exit

load-config
if [[ "$MODE" == sym ]]; then
    . $(dirname "$0")/lib-sym
    setup-kllvm
    inject-sym-flags
    inject-compilers
else
    . $(dirname "$0")/lib-dyn
    setup-allvm
    inject-dyn-flags
    inject-compilers
fi

run-program "$@"
